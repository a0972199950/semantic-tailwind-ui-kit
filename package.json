{
  "name": "fundfluent-frontend-design-system",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/fundfluent-frontend-design-system/fundfluent-frontend-design-system.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build-semantic": "cd src/semantic && gulp build-css build-assets",
    "watch-semantic": "cd src/semantic && yarn run build-semantic && gulp watch",
    "build": "stencil build --docs",
    "dev": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "^2.6.0",
    "fomantic-ui": "^2.8.7",
    "postcss": "^8.1.14",
    "react": "^17.0.2"
  },
  "license": "MIT",
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@stencil/less": "^1.0.0",
    "@stencil/postcss": "^2.0.0",
    "@stencil/react-output-target": "0.0.9",
    "@stencil/sass": "^1.4.1",
    "autoprefixer": "^10.2.6",
    "cssnano": "^5.0.5",
    "less-plugin-autoprefix": "^2.0.0",
    "postcss-replace": "^1.1.3",
    "semantic-ui-react": "^2.0.3",
    "tailwindcss": "^2.1.4"
  }
}
